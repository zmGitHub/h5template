.browsehappy {
  margin: 0.2em 0;
  background: #ccc;
  color: #000;
  padding: 0.2em 0;
}

html, body {
  padding: 0;
  height: 100%;
  min-height: 100%;
}
body {
    background: #f2f2f2;
  color: #212121;
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
}

fieldset{
  border:none;
}

md-sidenav.md-locked-open{
	min-width: 260px;
	max-width: 260px;
}
header md-toolbar{
  background: #4285f4;
  color: #fff;
  min-height: 56px;
}
header md-toolbar ,header .md-toolbar-tools{
  height: 56px;
}
header md-toolbar .md-menu {
  padding:4px 0;
}

.header{
	background: #fff;
  	color: rgba(49, 53, 52, 0.6);
  	box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.33);
}
.badge {
  display: inline-block;
  min-width: 10px;
  padding: 3px 7px;
  font-size: 12px;
  font-weight: bold;
  color: #ffffff;
  line-height: 1;
  vertical-align: baseline;
  white-space: nowrap;
  text-align: center;
  background-color: #969c9c;
  border-radius: 10px;
  position: absolute;
  font-size: 10px;
  top: 5px;
  right:4px;
}
sup.badge {
  margin-left: -1px;
  margin-right: -1px;
}
.badge.style-danger {
  background-color: #f44336;
  color: #ffffff;
  border-color: #f44336;
}


md-toolbar.header md-icon,md-toolbar.header .md-button{
	color: rgba(49, 53, 52, 0.6);
}

.full-height{
  height: 100%;
}


